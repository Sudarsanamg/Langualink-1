{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    
    "rewrites": [{ "source": "/(.*)", "destination": "/" }] ,
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ]

  }
  